{"ast":null,"code":"import FileUpload from '@/components/FileUpload.vue';\nimport ChatBox from '@/components/ChatBox.vue';\nexport default {\n  name: 'HomePage',\n  components: {\n    FileUpload,\n    ChatBox\n  },\n  data() {\n    return {\n      username: 'ÁÆ°ÁêÜÂëò',\n      userAvatar: '',\n      isAdmin: false,\n      knowledgeBases: [],\n      currentKb: null,\n      currentKbDetails: null,\n      currentChatId: null,\n      kbSearchQuery: '',\n      globalSearch: '',\n      searchScope: 'all',\n      loading: false,\n      editDialogVisible: false,\n      uploadDialogVisible: false,\n      userMessage: '',\n      selectedModel: 'deepseek-v3',\n      webSearchEnabled: false,\n      editForm: {\n        id: null,\n        name: '',\n        description: ''\n      },\n      editType: 'create',\n      suggestedQuestions: [{\n        text: 'ÂÖ≥‰∫éÂä≥Âä®‰∫âËÆÆÊ°à‰ª∂ÈÄÇÁî®Ê≥ïÂæãÁöÑÊúÄÊñ∞Ëß£Èáä'\n      }, {\n        text: 'Êí∞ÂÜô‰∏Ä‰∏™„ÄåÊñ∞Èóª‰ª∑ÂÄº„ÄçÁöÑÂêçËØçËß£Èáä'\n      }, {\n        text: 'ÂàÜÊâã‰∫ÜÔºåÊÅãÁà±ÊúüÈó¥ÁöÑËµ†‰∏éËøòËÉΩË¶ÅÂõûÂêó?'\n      }]\n    };\n  },\n  computed: {\n    editDialogTitle() {\n      return this.editType === 'create' ? 'Êñ∞Âª∫Áü•ËØÜÂ∫ì' : 'ÁºñËæëÁü•ËØÜÂ∫ì';\n    },\n    filteredKnowledgeBases() {\n      if (!this.kbSearchQuery) {\n        return this.knowledgeBases;\n      }\n      return this.knowledgeBases.filter(kb => kb.name.toLowerCase().includes(this.kbSearchQuery.toLowerCase()) || kb.description && kb.description.toLowerCase().includes(this.kbSearchQuery.toLowerCase()));\n    }\n  },\n  created() {\n    this.initUserData();\n    this.fetchKnowledgeBases();\n  },\n  methods: {\n    initUserData() {\n      const userData = localStorage.getItem('user');\n      if (userData) {\n        const user = JSON.parse(userData);\n        this.username = user.username || 'ÁÆ°ÁêÜÂëò';\n        this.isAdmin = user.is_admin || false;\n      }\n    },\n    async fetchKnowledgeBases() {\n      this.loading = true;\n      try {\n        // Ê®°ÊãüAPIË∞ÉÁî®Ëé∑ÂèñÁü•ËØÜÂ∫ìÂàóË°®\n        await new Promise(resolve => setTimeout(resolve, 800));\n\n        // Ê®°ÊãüÊï∞ÊçÆ\n        this.knowledgeBases = [{\n          id: 1,\n          name: 'Ê≥ïÂæãÊ≥ïËßÑÁü•ËØÜÂ∫ì',\n          file_count: 8,\n          description: 'ÂÖ≥‰∫éÂä≥Âä®‰∫âËÆÆÊ°à‰ª∂ÈÄÇÁî®Ê≥ïÂæãÁöÑÊúÄÊñ∞Ëß£Èáä',\n          update_time: '2023-06-15T08:30:00Z',\n          files: [{\n            id: 101,\n            name: 'Âä≥Âä®Ê≥ïËß£Èáä.pdf',\n            type: 'pdf',\n            size: 2456789,\n            upload_time: '2023-06-15T08:30:00ÊûÅÈÄüÈóÆËØäZ'\n          }, {\n            id: 102,\n            name: 'Âä≥Âä®ÂêàÂêåËåÉÊú¨.docx',\n            type: 'docx',\n            size: 12456,\n            upload_time: '2023-06-18T14:20:00Z'\n          }]\n        }, {\n          id: 2,\n          name: 'Êñ∞Èóª‰º†Êí≠Â≠¶ËµÑÊñôÂ∫ì',\n          file_count: 12,\n          description: 'Êí∞ÂÜô‰∏Ä‰∏™„ÄåÊñ∞Èóª‰ª∑ÂÄº„ÄçÁöÑÂêçËØçËß£Èáä',\n          update_time: '2023-06-20T10:45:00Z',\n          files: [{\n            id: 201,\n            name: 'Êñ∞ÈóªÂ≠¶ÁêÜËÆ∫.pdf',\n            type: 'pdf',\n            size: 3456789,\n            upload_time: '2023-06-20T10:45:00Z'\n          }]\n        }, {\n          id: 3,\n          name: 'ÂÖ¨ÁõäÊ≥ïÂæãÊè¥Âä©Êô∫Â∫ì',\n          file_count: 5,\n          description: 'ÂàÜÊâã‰∫ÜÔºåÊÅãÁà±ÊúüÈó¥ÁöÑËµ†‰∏éËøòËÉΩË¶ÅÂõûÂêó?',\n          update_time: '2023-06-22T16:15:00Z',\n          files: []\n        }];\n        if (this.knowledgeBases.length > 0 && !this.currentKb) {\n          this.currentKb = this.knowledgeBases[0].id;\n          this.currentKbDetails = this.knowledgeBases[0];\n        }\n      } catch (error) {\n        this.$message.error('Ëé∑ÂèñÁü•ËØÜÂ∫ìÂ§±Ë¥•: ' + (error.message || 'Êú™Áü•ÈîôËØØ'));\n      } finally {\n        this.loading = false;\n      }\n    },\n    selectKb(kb) {\n      this.currentKb = kb.id;\n      this.currentKbDetails = kb;\n      this.currentChatId = null;\n    },\n    createKB() {\n      this.editType = 'create';\n      this.editForm = {\n        id: null,\n        name: '',\n        description: ''\n      };\n      this.editDialogVisible = true;\n    },\n    confirmEdit() {\n      if (!this.editForm.name.trim()) {\n        this.$message.warning('ËØ∑ËæìÂÖ•Áü•ËØÜÂ∫ìÂêçÁß∞');\n        return;\n      }\n      if (this.editType === 'create') {\n        // ÂàõÂª∫Êñ∞Áü•ËØÜÂ∫ì\n        const newKb = {\n          id: Date.now(),\n          name: this.editForm.name,\n          description: this.editForm.description,\n          file_count: 0,\n          files: [],\n          update_time: new Date().toISOString()\n        };\n        this.knowledgeBases.push(newKb);\n        this.currentKb = newÊûÅÈÄüÈóÆËØäKb.id;\n        this.currentKbDetails = newKb;\n        this.$message.success(`Áü•ËØÜÂ∫ì\"${newKb.name}\"ÂàõÂª∫ÊàêÂäü`);\n      } else {\n        // ÁºñËæëÁé∞ÊúâÁü•ËØÜÂ∫ì\n        const kbIndex = this.knowledgeBases.findIndex(kb => kb.id === this.editForm.id);\n        if (kbIndex !== -1) {\n          this.knowledgeBases[kbIndex].name = this.editForm.name;\n          this.knowledgeBases[kbIndex].description = this.editForm.description;\n          if (this.currentKb === this.editForm.id) {\n            this.currentKbDetails = this.knowledgeBases[kbIndex];\n          }\n          this.$message.success('Áü•ËØÜÂ∫ìÊõ¥Êñ∞ÊàêÂäü');\n        }\n      }\n      this.editDialogVisible = false;\n    },\n    handleUploadSuccess(fileData) {\n      if (!this.currentKbDetails.files) {\n        this.currentKÊûÅÈÄüÈóÆËØäbDetails.files = [];\n      }\n      this.currentKbDetails.files.push({\n        id: Date.now(),\n        name: fileData.filename,\n        type: fileData.type || 'unknown',\n        size: fileData.size || 0,\n        upload_time: new Date().toISOString()\n      });\n      this.currentKbDetails.file_count = this.currentKbDetails.files.length;\n      this.uploadDialogVisible = false;\n      this.$message.success(`Êñá‰ª∂\"${fileData.filename}\"‰∏ä‰º†ÊàêÂäü`);\n    },\n    formatSize(bytes) {\n      if (bytes === 0) return '0 B';\n      const k = 1024;\n      const sizes = ['B', 'KB', 'MB', 'GB'];\n      const i = Math.floor(Math.log(bytes) / Math.log(k));\n      return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + ' ' + sizes[i];\n    },\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      return `${date.getFullYear()}-${(date.getMonth() + 1).toString().padStart(2, '0')}-${date.getDate().toString().padStart(2, '0')}`;\n    },\n    formatRelativeTime(dateString) {\n      const date = new Date(dateString);\n      const now = new Date();\n      const diffTime = Math.abs(now - date);\n      const diffDays = Math.ceil(diffTime / (1000 * 60 * 60 * 24));\n      if (diffDays === 1) return '‰ªäÂ§©';\n      if (diffDays === 2) return 'Êò®Â§©';\n      if (diffDays < 7) return `${diffDays}Â§©Ââç`;\n      if (diffDays < 30) return `${Math.floor(diffDays / 7)}Âë®Ââç`;\n      return this.formatDate(dateString);\n    },\n    downloadFile(file) {\n      this.$message.info(`ÂºÄÂßã‰∏ãËΩΩ: ${file.name}`);\n    },\n    previewFile(file) {\n      this.$message.info(`È¢ÑËßàÊñá‰ª∂: ${file.name}`);\n    },\n    showUploadDialog() {\n      if (!this.currentKb) {\n        this.$message.warning('ËØ∑ÂÖàÈÄâÊã©‰∏Ä‰∏™Áü•ËØÜÂ∫ì');\n        return;\n      }\n      this.uploadDialogVisible = true;\n    },\n    sendMessage() {\n      if (!this.userMessage.trim()) return;\n      if (!this.currentChatId) {\n        this.currentChatId = 'chat_' + Date.now();\n      }\n      this.userMessage = '';\n    },\n    askSuggestedQuestion(question) {\n      this.userMessage = question;\n      this.sendMessage();\n    },\n    goToProfile() {\n      this.$message.info('ÊâìÂºÄ‰∏™‰∫∫‰∏≠ÂøÉ');\n    },\n    goToAdmin() {\n      this.$router.push('/admin');\n    },\n    logout() {\n      this.$confirm('Á°ÆÂÆöË¶ÅÈÄÄÂá∫ÁôªÂΩïÂêó?', 'ÊèêÁ§∫', {\n        confirmButtonText: 'Á°ÆÂÆö',\n        cancelButtonText: 'ÂèñÊ∂à',\n        type: 'info'\n      }).then(() => {\n        localStorage.removeItem('token');\n        localStorage.removeItem('user');\n        if (this.$store) {\n          this.$store.commit('setToken', null);\n          this.$store.commit('setUser', null);\n        }\n        this.$router.push('/login');\n        this.$message.success('ÊÇ®Â∑≤ÊàêÂäüÈÄÄÂá∫ÁôªÂΩï');\n      }).catch(() => {});\n    }\n  }\n};","map":{"version":3,"names":["FileUpload","ChatBox","name","components","data","username","userAvatar","isAdmin","knowledgeBases","currentKb","currentKbDetails","currentChatId","kbSearchQuery","globalSearch","searchScope","loading","editDialogVisible","uploadDialogVisible","userMessage","selectedModel","webSearchEnabled","editForm","id","description","editType","suggestedQuestions","text","computed","editDialogTitle","filteredKnowledgeBases","filter","kb","toLowerCase","includes","created","initUserData","fetchKnowledgeBases","methods","userData","localStorage","getItem","user","JSON","parse","is_admin","Promise","resolve","setTimeout","file_count","update_time","files","type","size","upload_time","length","error","$message","message","selectKb","createKB","confirmEdit","trim","warning","newKb","Date","now","toISOString","push","newÊûÅÈÄüÈóÆËØäKb","success","kbIndex","findIndex","handleUploadSuccess","fileData","currentKÊûÅÈÄüÈóÆËØäbDetails","filename","formatSize","bytes","k","sizes","i","Math","floor","log","parseFloat","pow","toFixed","formatDate","dateString","date","getFullYear","getMonth","toString","padStart","getDate","formatRelativeTime","diffTime","abs","diffDays","ceil","downloadFile","file","info","previewFile","showUploadDialog","sendMessage","askSuggestedQuestion","question","goToProfile","goToAdmin","$router","logout","$confirm","confirmButtonText","cancelButtonText","then","removeItem","$store","commit","catch"],"sources":["src/views/apps.vue"],"sourcesContent":["<template>\r\n  <div class=\"home-container\">\r\n    <!-- È°∂ÈÉ®ÂØºËà™Ê†è -->\r\n    <div class=\"app-header\">\r\n      <div class=\"header-content\">\r\n        <div class=\"logo-section\">\r\n          <span class=\"logo-icon\">üìö</span>\r\n          <h1 class=\"app-title\">Êô∫ËÉΩÁü•ËØÜÂ∫ìÂä©Êâã</h1>\r\n        </div>\r\n        \r\n        <div class=\"search-section\">\r\n          <el-select v-model=\"searchScope\" class=\"scope-select\">\r\n            <el-option label=\"ÂÖ®ÈÉ®\" value=\"all\"></el-option>\r\n            <el-option label=\"Áü•ËØÜÂ∫ì\" value=\"kb\"></el-option>\r\n            <el-option label=\"Êñá‰ª∂\" value=\"files\"></el-option>\r\n          </el-select>\r\n          <el-input\r\n            v-model=\"globalSearch\"\r\n            placeholder=\"ÊêúÁ¥¢Áü•ËØÜÂ∫ìÂÜÖÂÆπ...\"\r\n            class=\"global-search\"\r\n            clearable\r\n          >\r\n            <template #prefix>\r\n              <i class=\"el-icon-search\"></i>\r\n            </template>\r\n          </el-input>\r\n        </div>\r\n        \r\n        <div class=\"user-section\">\r\n          <el-dropdown>\r\n            <div class=\"user-info\">\r\n              <el-avatar :size=\"32\" :src=\"userAvatar\"></el-avatar>\r\n              <span class=\"username\">{{ username }}</span>\r\n            </div>\r\n            <template #dropdown>\r\n              <el-dropdown-menu>\r\n                <el-dropdown-item @click=\"goToProfile\">\r\n                  <i class=\"el-icon-user\"></i> ‰∏™‰∫∫‰∏≠ÂøÉ\r\n                </el-dropdown-item>\r\n                <el-dropdown-item @click=\"goToAdmin\" v-if=\"isAdmin\">\r\n                  <i class=\"el-icon-settings\"></i> ÁÆ°ÁêÜÂêéÂè∞\r\n                </el-dropdown-item>\r\n                <el-dropdown-item divided @click=\"logout\">\r\n                  <i class=\"el-icon-switch-button\"></i> ÈÄÄÂá∫ÁôªÂΩï\r\n                </el-dropdown-item>\r\n              </el-dropdown-menu>\r\n            </template>\r\n          </el-dropdown>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"main-content\">\r\n      <!-- Â∑¶‰æßÁü•ËØÜÂ∫ìÁÆ°ÁêÜÂå∫Âüü -->\r\n      <div class=\"sidebar\">\r\n        <div class=\"sidebar-header\">\r\n          <h2>Áü•ËØÜÂ∫ì</h2>\r\n          <el-button \r\n            type=\"primary\" \r\n            icon=\"el-icon-plus\" \r\n            @click=\"createKB\"\r\n            size=\"small\"\r\n            class=\"create-btn\">\r\n            Êñ∞Âª∫\r\n          </el-button>\r\n        </div>\r\n        \r\n        <div class=\"search-box\">\r\n          <el-input\r\n            placeholder=\"ÊêúÁ¥¢Áü•ËØÜÂ∫ì...\"\r\n            prefix-icon=\"el-icon-search\"\r\n            v-model=\"kbSearchQuery\"\r\n            clearable\r\n            size=\"small\"\r\n          ></el-input>\r\n        </div>\r\n        \r\n        <div class=\"kb-list\">\r\n          <div \r\n            v-for=\"kb in filteredKnowledgeBases\" \r\n            :key=\"kb.id\" \r\n            class=\"kb-item\"\r\n            :class=\"{ active: currentKb === kb.id }\"\r\n            @click=\"selectKb(kb)\">\r\n            <div class=\"kb-icon\">\r\n              <i class=\"el-icon-folder\"></i>\r\n            </div>\r\n            <div class=\"kb-info\">\r\n              <div class=\"kb-name\">{{ kb.name }}</div>\r\n              <div class=\"kb-desc\">{{ kb.description || 'ÊöÇÊó†ÊèèËø∞' }}</div>\r\n              <div class=\"kb-meta\">\r\n                <span class=\"file-count\">{{ kb.file_count }}‰∏™Êñá‰ª∂</span>\r\n                <span class=\"update-time\">Êõ¥Êñ∞‰∫é{{ formatRelativeTime(kb.update_time) }}</span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        \r\n        <div class=\"empty-state\" v-if=\"knowledgeBases.length === 0\">\r\n          <div class=\"empty-icon\">üìÅ</div>\r\n          <p class=\"empty-text\">ÊÇ®ËøòÊ≤°ÊúâÂàõÂª∫‰ªª‰ΩïÁü•ËØÜÂ∫ì</p>\r\n          <el-button type=\"primary\" @click=\"createKB\">ÂàõÂª∫Áü•ËØÜÂ∫ì</el-button>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- ‰∏≠Èó¥ËÅäÂ§©Âå∫Âüü -->\r\n      <div class=\"chat-area\">\r\n        <div class=\"chat-header\">\r\n          <div class=\"current-kb-info\" v-if=\"currentKbDetails\">\r\n            <div class=\"kb-badge\">\r\n              <i class=\"el-icon-folder\"></i>\r\n              <span>{{ currentKbDetails.name }}</span>\r\n            </div>\r\n            <el-tag size=\"small\">\r\n              {{ currentKbDetails.file_count }}‰∏™Êñá‰ª∂\r\n            </el-tag>\r\n          </div>\r\n        </div>\r\n        \r\n        <div class=\"chat-container\">\r\n          <div class=\"welcome-screen\" v-if=\"!currentChatId\">\r\n            <div class=\"welcome-content\">\r\n              <div class=\"welcome-icon\">üí¨</div>\r\n              <h2>Ê¨¢Ëøé‰ΩøÁî®Êô∫ËÉΩÁü•ËØÜÂ∫ìÂä©Êâã</h2>\r\n              <p>ÈÄâÊã©ÊàñÂàõÂª∫‰∏Ä‰∏™Áü•ËØÜÂ∫ìÂºÄÂßãÊô∫ËÉΩÂØπËØù</p>\r\n              <div class=\"suggested-questions\">\r\n                <div \r\n                  v-for=\"(question, index) in suggestedQuestions\" \r\n                  :key=\"index\" \r\n                  class=\"question-card\"\r\n                  @click=\"askSuggestedQuestion(question.text)\">\r\n                  {{ question.text }}\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          \r\n          <div class=\"chat-main\" v-else>\r\n            <ChatBox :kb-id=\"currentKb\" :chat-id=\"currentChatId\" class=\"chat-box\"/>\r\n          </div>\r\n        </div>\r\n        \r\n        <div class=\"input-area\">\r\n          <div class=\"input-container\">\r\n            <el-input\r\n              v-model=\"userMessage\"\r\n              placeholder=\"ËæìÂÖ•ÈóÆÈ¢òÊàñ@Áü•ËØÜÂ∫ìËøõË°åÊèêÈóÆ\"\r\n              @keyup.enter.native=\"sendMessage\"\r\n              class=\"message-input\"\r\n            >\r\n              <template #append>\r\n                <el-button \r\n                  icon=\"el-icon-s-promotion\" \r\n                  @click=\"sendMessage\"\r\n                  :disabled=\"!userMessage.trim()\"\r\n                ></el-button>\r\n              </template>\r\n            </el-input>\r\n          </div>\r\n          <div class=\"input-options\">\r\n            <el-select v-model=\"selectedModel\" size=\"small\" placeholder=\"Ê®°Âûã\">\r\n              <el-option label=\"DeepSeek V3\" value=\"deepseek-v3\"></el-option>\r\n              <el-option label=\"GPT-4\" value=\"gpt-4\"></el-option>\r\n              <el-option label=\"Claude 3\" value=\"claude-ÊûÅÈÄüÈóÆËØä3\"></el-option>\r\n            </el-select>\r\n            <el-switch\r\n              v-model=\"webSearchEnabled\"\r\n              active-text=\"ËÅîÁΩëÊêúÁ¥¢\"\r\n              size=\"small\">\r\n            </el-switch>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!-- \r\n      \r\n      -->\r\n\r\n      <!-- Âè≥‰æßÊñá‰ª∂ÁÆ°ÁêÜÂå∫Âüü -->\r\n      <div class=\"file-sidebar\">\r\n        <div class=\"sidebar-header\">\r\n          <h2>Êñá‰ª∂ÁÆ°ÁêÜ</h2>\r\n          <el-button \r\n            type=\"primary\" \r\n            icon=\"el-icon-upload\" \r\n            @click=\"showUploadDialog\"\r\n            size=\"small\"\r\n            :disabled=\"!currentKb\"\r\n          >\r\n            ‰∏ä‰º†\r\n          </el-button>\r\n        </div>\r\n        \r\n        <div class=\"file-list\">\r\n          <div \r\n            v-for=\"file in currentKbDetails.files\" \r\n            :key=\"file.id\" \r\n            class=\"file-item\"\r\n            @dblclick=\"previewFile(file)\">\r\n            <div class=\"file-icon\">\r\n              <i class=\"el-icon-document\"></i>\r\n            </div>\r\n            <div class=\"file-info\">\r\n              <div class=\"file-name\">{{ file.name }}</div>\r\n              <div class=\"file-meta\">\r\n                <span>{{ formatSize(file.size) }}</span>\r\n                <span>‚Ä¢</span>\r\n                <span>{{ formatDate(file.upload_time) }}</span>\r\n              </div>\r\n            </div>\r\n            <div class=\"file-actions\">\r\n              <el-tooltip content=\"È¢ÑËßà\">\r\n                <el-button icon=\"el-icon-view\" circle size=\"mini\" @click=\"previewFile(file)\"></el-button>\r\n              </el-tooltip>\r\n              <el-tooltip content=\"‰∏ãËΩΩ\">\r\n                <el-button iconÊûÅÈÄüÈóÆËØä=\"el-icon-download\" circle size=\"mini\" @click=\"downloadFile(file)\"></el-button>\r\n              </el-tooltip>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        \r\n        <div class=\"empty-state\" v-if=\"!currentKbDetails.files || currentKbDetails.files.length === 0\">\r\n          <div class=\"empty-icon\">üìÑ</div>\r\n          <p class=\"empty-text\">ÂΩìÂâçÁü•ËØÜÂ∫ìÊöÇÊó†Êñá‰ª∂</p>\r\n          <el-button type=\"primary\" @ÊûÅÈÄüÈóÆËØäclick=\"showUploadDialog\">‰∏ä‰º†Êñá‰ª∂</el-button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    \r\n    <!-- ‰∏ä‰º†Êñá‰ª∂ÂØπËØùÊ°Ü -->\r\n    <el-dialog\r\n      title=\"‰∏ä‰º†Êñá‰ª∂\"\r\n      :visible.sync=\"uploadDialogVisible\"\r\n      width=\"500px\">\r\n      <FileUpload :kb-id=\"currentKb\" @uploaded=\"handleUploadSuccess\"/>\r\n    </el-dialog>\r\n    \r\n    <!-- Áü•ËØÜÂ∫ìÁºñËæëÂØπËØùÊ°Ü -->\r\n    <el-dialog\r\n      :title=\"editDialogTitle\"\r\n      :visible.sync=\"editDialogVisible\"\r\n      width=\"500px\">\r\n      <el-form :model=\"editForm\" label-width=\"80px\">\r\n        <el-form-item label=\"ÂêçÁß∞\">\r\n          <el-input v-model=\"editForm.name\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"ÊèèËø∞\">\r\n          <el-input \r\n            type=\"textarea\" \r\n            v-model=\"editForm.description\" \r\n            :autosize=\"{ minRows: 2, maxRows: 4 }\"\r\n            placeholder=\"ËØ∑ËæìÂÖ•Áü•ËØÜÂ∫ìÊèèËø∞\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"editDialogVisible = false\">ÂèñÊ∂à</el-button>\r\n        <el-button type=\"primary\" @click=\"c