JSON.parse(userData);\r\n        this.username = user.username || 'ÁÆ°ÁêÜÂëò';\r\n        this.isAdmin = user.is_admin || false;\r\n      }\r\n    },\r\n    \r\n    async fetchKnowledgeBases() {\r\n      this.loading = true;\r\n      try {\r\n        // Ê®°ÊãüAPIË∞ÉÁî®Ëé∑ÂèñÁü•ËØÜÂ∫ìÂàóË°®\r\n        await new Promise(resolve => setTimeout(resolve, 800));\r\n        \r\n        // Ê®°ÊãüÊï∞ÊçÆ\r\n        this.knowledgeBases = [\r\n          { \r\n            id: 1, \r\n            name: 'Ê≥ïÂæãÊ≥ïËßÑÁü•ËØÜÂ∫ì', \r\n            file_count: 8, \r\n            description: 'ÂÖ≥‰∫éÂä≥Âä®‰∫âËÆÆÊ°à‰ª∂ÈÄÇÁî®Ê≥ïÂæãÁöÑÊúÄÊñ∞Ëß£Èáä',\r\n            update_time: '2023-06-15T08:30:00Z',\r\n            files: [\r\n              { id: 101, name: 'Âä≥Âä®Ê≥ïËß£Èáä.pdf', type: 'pdf', size: 2456789, upload_time: '2023-06-15T08:30:00ÊûÅÈÄüÈóÆËØäZ' },\r\n              { id: 102, name: 'Âä≥Âä®ÂêàÂêåËåÉÊú¨.docx', type: 'docx', size: 12456, upload_time: '2023-06-18T14:20:00Z' }\r\n            ]\r\n          },\r\n          { \r\n            id: 2, \r\n            name: 'Êñ∞Èóª‰º†Êí≠Â≠¶ËµÑÊñôÂ∫ì', \r\n            file_count: 12, \r\n            description: 'Êí∞ÂÜô‰∏Ä‰∏™„ÄåÊñ∞Èóª‰ª∑ÂÄº„ÄçÁöÑÂêçËØçËß£Èáä',\r\n            update_time: '2023-06-20T10:45:00Z',\r\n            files: [\r\n              { id: 201, name: 'Êñ∞ÈóªÂ≠¶ÁêÜËÆ∫.pdf', type: 'pdf', size: 3456789, upload_time: '2023-06-20T10:45:00Z' }\r\n            ]\r\n          },\r\n          { \r\n            id: 3, \r\n            name: 'ÂÖ¨ÁõäÊ≥ïÂæãÊè¥Âä©Êô∫Â∫ì', \r\n            file_count: 5, \r\n            description: 'ÂàÜÊâã‰∫ÜÔºåÊÅãÁà±ÊúüÈó¥ÁöÑËµ†‰∏éËøòËÉΩË¶ÅÂõûÂêó?',\r\n            update_time: '2023-06-22T16:15:00Z',\r\n            files: []\r\n          }\r\n        ];\r\n        \r\n        if (this.knowledgeBases.length > 0 && !this.currentKb) {\r\n          this.currentKb = this.knowledgeBases[0].id;\r\n          this.currentKbDetails = this.knowledgeBases[0];\r\n        }\r\n      } catch (error) {\r\n        this.$messa